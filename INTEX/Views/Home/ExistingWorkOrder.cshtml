
@{
    ViewBag.Title = "ExistingWorkOrder";
    Layout = "~/Views/Shared/_CustomerLayoutPage.cshtml";
}
<link href="~/Content/NorthwestLabsStyleSheet.css" rel="stylesheet" />

<div class="container" id="existingworkorder">
    <h2>Existing Work Order</h2>
</div>

<!--WORK ORDER PROGRESS-->

<div class="container">
    <div class="row">
        <h2 class="text-center">Work Order #96<br /><small>Hover over the options if you can't see the text</small></h2>
        <h3 class="text-center">Assay #899483</h3>
        <hr />
    </div>

    <div class="row">
        <div class="col-sm-offset-4 col-sm-4">
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;" data-toggle="tooltip" data-placement="top" title="Biochemical Pharmacology (BP)">
                    <span class="sr-only">60% Complete</span>
                    <span class="progress-type">Biochemical Pharmacology (BP)</span>
                </div>
            </div>
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="1" aria-valuemin="0" aria-valuemax="100" style="width: 80%;" data-toggle="tooltip" data-placement="top" title="DiscoveryScreen® (DS)">
                    <span class="sr-only">80% Complete</span>
                    <span class="progress-type">DiscoveryScreen® (DS)</span>
                </div>
            </div>
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="1" aria-valuemin="0" aria-valuemax="100" style="width: 25%;" data-toggle="tooltip" data-placement="top" title="ImmunoScreen® (IS)">
                    <span class="sr-only">1% Complete</span>
                    <span class="progress-type">ImmunoScreen® (IS)</span>
                </div>
            </div>
            <div class="progress-meter">
                <div class="meter meter-left" style="width: 25%;"><span class="meter-text">In queue</span></div>
                <div class="meter meter-left" style="width: 25%;"><span class="meter-text">In progess</span></div>
                <div class="meter meter-right" style="width: 30%;"><span class="meter-text">Completed</span></div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(function () {
        $('[data-toggle="tooltip"]').tooltip();
    });
</script>